def extract_zip(self):
    """Extrait ZIP dans volume Unity Catalog"""
    print("ğŸ“¦ Extraction ZIP...")
    
    zip_path = self.config.zip_path
    extract_dir = self.config.extract_dir
    
    # âœ… ADD THIS CHECK
    if not os.path.exists(zip_path):
        available_files = []
        input_dir = os.path.dirname(zip_path)
        if os.path.exists(input_dir):
            available_files = os.listdir(input_dir)
        
        raise FileNotFoundError(
            f"âŒ ZIP file not found: {zip_path}\n"
            f"ğŸ“‚ Input directory: {input_dir}\n"
            f"ğŸ“„ Available files: {available_files if available_files else 'Directory is empty or does not exist'}\n"
            f"ğŸ’¡ Please upload wax_data.zip to the input directory"
        )
    
    # Create extraction directory
    os.makedirs(extract_dir, exist_ok=True)
    
    # Extract
    with zipfile.ZipFile(zip_path, 'r') as zip_ref:
        zip_ref.extractall(extract_dir)
    
    print(f"âœ… ZIP extrait : {extract_dir}")
